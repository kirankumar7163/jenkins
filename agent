pipeline {

  agent {
    label 'JenkinsAgent'
  }

  stages {

    stage('Hello') {
      steps {
        echo 'Hello World'
      }
    }

  }

  post {
    always {
      echo "sending email"
    }
  }

}

@Library('roboshop') _

pipeline {
  agent any
  stages {
    stage ('test') {
      steps {
        script {
        def abc = "hello"
        def xyz = 10

        print "abc = ${abc}"
        print "xyz = ${xyz}"

        print abc


        }
      }

    }
  }

}