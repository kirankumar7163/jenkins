pipeline {

  agent {
    label 'JenkinsAgent'
  }

  stages {

    stage('Hello') {
      steps {
        echo 'Hello World'
      }
    }

  }

  post {
    always {
      echo "sending email"
    }
  }

}

@Library('roboshop') _
pipeline {
  agent any
   stages {
      stage('test') {
        steps {
          script {
            env.abc = "Hello"
            def xyz = 10
            def x1 = true

            print "abc = ${abc}"
            print "xyz = ${xyz}"

            print abc

        }
      }

    }
  }
}

